<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>

  <style>
    table{
      border-collapse: collapse;
      border-spacing: 0;
    }
    th, td{
      text-align: center;
      padding: 10px 20px;
      border: 1px solid #000;
    }
</style>

</head>
<body>
  <h1>scoreList</h1>
  <table>
    <thead>
      <tr>
        <th>번호</th>
        <th>과목</th>
        <th>등급</th>
      </tr>
    </thead>
    <tbody>
      <% let index = 1;
      scoreList.forEach(score => { %>
        <tr>
          <td><%= index++ %></td>
          <td><%= score.name %></td>
          <td><%= score.grade %></td>
        </tr>
      <% }) %>
    </tbody>

  </table>
</body>
</html>